    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "bcryptjs": "^2.4.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
