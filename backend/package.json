{
  "name": "grove-backend",
  "version": "1.0.0",
  "description": "Enhanced Backend server for Augustine Grove Web Application with booking system",
  "main": "hotdog.js",
  "type": "commonjs",
  "scripts": {
    "start": "node hotdog.js",
    "dev": "nodemon hotdog.js",
    "test:smoke": "node --experimental-fetch ./tests/smoke.backend.mjs",
    "test:gallery": "node --experimental-fetch ./tests/smoke.gallery.mjs",
    "dev:frontend": "cd ../frontend && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.5",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "grove",
    "residential",
    "community",
    "nodejs",
    "express",
    "booking-system"
  ],
  "author": "Aser's Angels",
  "license": "MIT"
}
