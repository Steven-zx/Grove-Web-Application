{
  "name": "grove-web-application",
  "version": "2.0.0",
  "description": "Grove Community Web Application - Enhanced Version with Booking System",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && node hotdog.js",
    "build": "cd frontend && npm run build",
    "start": "cd backend && node hotdog.js",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "setup": "npm run install:all"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "keywords": [
    "residential",
    "community",
    "booking",
    "amenities",
    "management"
  ],
  "author": "Grove Community",
  "license": "MIT"
}
